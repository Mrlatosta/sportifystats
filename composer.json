{
    "name": "mrlatosta/sportifystats",
    "description": "Aplicacion web de estadisticas e informacion deportiva",
    "type": "project",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"  
        }
    },
    "scripts": {
        "start": "php -S 0.0.0.0:$PORT -t public"  
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
